{
    "manifest_version": 3,
    "name": "AI-Dentifier",
    "version": "1.0",
    "description": "A brief description of your extension.",
    "icons": {
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "icons/icon48.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage"
    ]
}